export PATH="/Users/nlandsberg/vlc/extras/tools/build/bin:/Users/nlandsberg/vlc/contrib/x86_64-apple-darwin15/bin:/Users/nlandsberg/vlc/extras/tools/build/bin:/Users/nlandsberg/.pyenv/bin:/usr/local/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/Applications/Wireshark.app/Contents/MacOS" LANG=C
